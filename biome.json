{
	"$schema": "https://biomejs.dev/schemas/1.4.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
		"indentStyle": "space",
		"lineWidth": 80
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single",
			"trailingComma": "es5"
		}
	},
	"linter": {
		"enabled": false,
		"rules": {
			"recommended": true
		}
	}
}
